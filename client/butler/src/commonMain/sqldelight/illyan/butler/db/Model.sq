CREATE TABLE Model (
    uuid TEXT PRIMARY KEY NOT NULL UNIQUE,
    author TEXT NOT NULL,
    description TEXT NOT NULL,
    name TEXT NOT NULL,
    type TEXT NOT NULL,
    greetingMessage TEXT NOT NULL
);

select:
SELECT * FROM Model WHERE uuid = ?;

upsert:
INSERT OR REPLACE INTO Model
VALUES ?;

delete:
DELETE FROM Model WHERE uuid = ?;

deleteAll:
DELETE FROM Model;
